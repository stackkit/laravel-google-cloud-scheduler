        }
    ],
    "require": {
        "ext-json": "*",
        "google/cloud-scheduler": "^1.4",
        "firebase/php-jwt": "^5.2",
        "phpseclib/phpseclib": "~2.0",
        "laravel/framework": "8.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^3.5 || ^3.6 || ^3.7 || ^3.8 || ^4.0 || ^5.0",
        "symfony/console": "^4.4|^5.0",
        "timacdonald/log-fake": "^1.6"
    },
    "autoload": {
        "psr-4": {
